<section class="container faq">
  <div class="faq__header">
    <h2 class="primary-title faq__primary-title">{{ faqSection.title }}</h2>
    <h3 class="secondary-title faq__secondary-title">
      {{ faqSection.subtitle }}
    </h3>
    <button class="button faq__button button--tertiary">
      {{ faqSection.buttonText }}
    </button>
  </div>
  <div class="faq__list">
    {% for item in faqSection.items %}
      <details class="faq__item">
        <summary class="faq__question">
          <h4 class="faq__text">{{ item.question }}</h4>
          <div class="faq__icon">
            <img class="faq__icon-img" src="{{ item.icon.src }}" alt="{{ item.icon.alt }}"></div>
          </summary>
          <p class="faq__answer">{{ item.answer }}</p>
        </details>
      {% endfor %}
    </div>
  </section>